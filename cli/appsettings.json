{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Console": {
    "LogLevel": {
      "Default": "None"
    },
    "IncludeScopes": false
  },
  "api_gateway": {
    "endpoint": "http://localhost:7000",
    "routing": {
      "postTransaction": "mem/transaction",
      "getTransactionId": "chain/transaction/{0}",
      "getSafeguardTransactions": "chain/safeguard"
    }
  },
  "network": {
    "environment": "testnet",
    "enabled_restAPI": false,
    "restApi_endpoint": "http://localhost:8001/", 
    "number_of_confirmations": 1
  }
}
