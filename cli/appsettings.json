{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Console": {
    "LogLevel": {
      "Default": "None"
    },
    "IncludeScopes": false
  },
  "api_gateway": {
    "endpoint": "http://localhost:7000",
    "routing": {
      "postTransaction": "api/transaction/mempool",
      "getTransactionId": "api/transaction/{0}",
      "getSafeguardTransactions": "api/blocks/safeguard"
    }
  },
  "network": {
    "environment": "testnet",
    "enabled_restAPI": false,
    "restApi_endpoint": "http://localhost:8001/"
  }
}
